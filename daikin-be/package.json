{
  "name": "daikin-be",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "zip": "bestzip deploy.zip dist/*",
    "build:zip": "nest build && yarn run zip",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm_use": "npx typeorm-ts-node-commonjs -d src/database/config_use.ts",
    "typeorm_live": "npx typeorm-ts-node-commonjs -d src/database/config_live.ts",
    "migration:generate": "yarn run typeorm_use migration:generate",
    "migration:run": "yarn run typeorm_use migration:run && yarn run typeorm_live migration:run",
    "migration:revert": "yarn run typeorm_use migration:revert && yarn run typeorm_live migration:revert",
    "seed:create": "ts-node node_modules/typeorm-extension/bin/cli.cjs seed:create -r src/database/seeds",
    "seed:run": "ts-node node_modules/typeorm-extension/bin/cli.cjs seed:run -d \"src/database/config_use.ts\" && ts-node node_modules/typeorm-extension/bin/cli.cjs seed:run -d \"src/database/config_live.ts\"",
    "db:create": "ts-node node_modules/typeorm-extension/bin/cli.cjs db:create -d \"src/database/config_use.ts\" && ts-node node_modules/typeorm-extension/bin/cli.cjs db:create -d \"src/database/config_live.ts\"",
    "db:drop": "ts-node node_modules/typeorm-extension/bin/cli.cjs db:drop -d \"src/database/config_use.ts\" && ts-node node_modules/typeorm-extension/bin/cli.cjs db:drop -d \"src/database/config_live.ts\"",
    "fresh": "yarn run db:drop && yarn run db:create && yarn run seed:run"
  },
  "dependencies": {
    "@azure/msal-node": "^2.6.0",
    "@azure/storage-blob": "^12.17.0",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "^9.0.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^9.0.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^9.0.0",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/swagger": "^7.4.2",
    "@nestjs/typeorm": "^10.0.2",
    "asposeslidescloud": "^24.8.0",
    "bestzip": "^2.2.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cloudconvert": "^2.3.6",
    "dayjs": "^1.11.10",
    "docxtemplater": "^3.48.0",
    "exceljs": "^4.4.0",
    "express": "^4.21.0",
    "joi": "^17.13.3",
    "mysql2": "^3.9.3",
    "nestjs-azure-storage-blob": "^0.0.3",
    "nestjs-typeorm-paginate": "^4.0.4",
    "passport": "^0.7.0",
    "passport-azure-ad": "^4.3.5",
    "pizzip": "^3.1.7",
    "pptx-automizer": "^0.4.8",
    "pptxgenjs": "^3.12.0",
    "qrcode": "^1.5.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.1",
    "typeorm": "^0.3.20",
    "typeorm-extension": "^3.2.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^9.0.0",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^16.0.0",
    "@types/passport-azure-ad": "^4.3.5",
    "@types/qrcode": "^1.5.5",
    "@types/supertest": "^2.0.11",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "28.1.3",
    "prettier": "^2.3.2",
    "source-map-support": "^0.5.20",
    "supertest": "^6.1.3",
    "ts-jest": "28.0.8",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
